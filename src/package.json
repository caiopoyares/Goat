{
  "name": "pink-project",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass -w sass/main.scss css/style.css",
    "build:sass": "node-sass sass/main.scss ../dist/css/style.css",
    "autoprefixer": "postcss -u autoprefixer -r ../dist/css/*",
    "imagemin": "imagemin img/* --out-dir=../dist/img -p",
    "minify:css": "cleancss -o ../dist/css/style.min.css css/style.css",
    "build": "npm run build:sass && npm run autoprefixer && npm run imagemin && npm run minify:css"
  },
  "author": "caio poyares",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "clean-css-cli": "^4.2.1",
    "imagemin-cli": "^4.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  },
  "dependencies": {}
}
